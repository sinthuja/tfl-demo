<?xml version="1.0" encoding="UTF-8"?>
<eventReceiver name="TwitterReceiver" statistics="enable" trace="enable" xmlns="http://wso2.org/carbon/eventreceiver">
    <from eventAdapterType="http">
        <property name="transports">all</property>
    </from>
    <mapping customMapping="disable" type="json"/>
    <to streamName="twitterInputStream" version="1.0.0"/>
</eventReceiver>
